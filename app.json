{
  "expo": {
    "name": "SysGI",
    "slug": "sysgi",
    "description": "Sistema de Gerenciamento de Infratores",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "primaryColor": "#DC143C",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./src/assets/icon-notify.png",
      "color": "#DC143C",
      "androidMode": "default",
      "androidCollapsedTitle": "n/a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.UNIFENAS.SysGI",
      "icon": "./src/assets/icon-ios.png",
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "package": "com.UNIFENAS.SysGI",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon-adaptive.png"
      },
      "icon": "./src/assets/icon.png",
      "versionCode": 1
    }
  }
}
