{
  "expo": {
    "name": "SysGI",
    "slug": "sysgi",
    "description": "Sistema de Gerenciamento de Infratores",
    "privacy": "public",
    "platforms": ["ios", "android"],
    "githubUrl": "https://github.com/AdrielRA/SysGI",
    "userInterfaceStyle": "light",
    "version": "1.0.1",
    "orientation": "portrait",
    "primaryColor": "#DC143C",
    "androidNavigationBar": {
      "backgroundColor": "#2A0000"
    },
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "icon": "./src/assets/icon.png",
    "notification": {
      "icon": "./src/assets/icon-notify.png",
      "color": "#DC143C",
      "androidMode": "collapse",
      "androidCollapsedTitle": "#{unread_notifications} novas notificações!"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "bundleIdentifier": "com.UNIFENAS.SysGI",
      "icon": "./src/assets/icon-ios.png",
      "supportsTablet": true
    },
    "android": {
      "useNextNotificationsApi": true,
      "googleServicesFile": "./google-services.json",
      "package": "com.UNIFENAS.SysGI",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon-adaptive.png",
        "backgroundColor": "#ffffff"
      },
      "icon": "./src/assets/icon.png",
      "versionCode": 2,
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.UNIFENAS.SysGI"
    }
  }
}
