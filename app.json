{
  "expo": {
    "name": "SysGI",
    "slug": "SysGI",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./src/assets/icon-notify.png",
      "color": "#ffffff",
      "androidMode": "default"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.UNIFENAS.SysGI",
      "supportsTablet": true
    },
    "android": {
      "package": "com.UNIFENAS.SysGI",
      "versionCode": 1
    }
  }
}
